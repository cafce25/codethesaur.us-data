{
  "meta": {
    "language": "rust",
    "language_version": "1",
    "language_name": "Rust"
  },
  "concepts": {
    "if_conditional": {
      "code": "if condition {\n    statements;\n}"
    },
    "if_else_conditional": {
      "code": "if condition {\n    statements;\n} else {\n    statements;\n}"
    },
    "if_elseif_conditional": {
      "code": "if condition {\n    statements;\n} else if condition2 {\n    statements;\n}"
    },
    "if_elseif_else_conditional": {
      "code": "if condition {\n    statements;\n} else if condition2 {\n    statements;\n} else {\n    statements;\n}"
    },
    "ternary_conditional": {
      "code": "if condition { expression } else { expression }"
    },
    "switch_statement": {
      "code": "match expression {\n    pattern => { statements }\n    _ => { statements } // the special pattern _ matches all cases\n}"
    },
    "while_loop": {
      "code": "while condition {\n    statements;\n}"
    },
    "foreach_loop": {
      "code": "for variable in iterable {\n    statements;\n}"
    },
    "each_iteration": {
      "code": "iter.for_each(|item| {\n    statements;\n});"
    },
    "map_iteration": {
      "code": "iter.map(|item| {\n    expression\n}"
    },
    "filter_iteration": {
      "code": "iter.filter(|item| {\n    boolean_expression\n}"
    },
    "fold_iteration": {
      "code": "iter.fold(init, |acc, x| {\n    expression\n}"
    }
  }
}
