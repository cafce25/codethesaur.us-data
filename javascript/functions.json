{
  "meta": {
    "language": "javascript",
    "language_version": "ES2020",
    "language_name": "JavaScript"
  },
  "categories": {
    "Void Functions": [
      "void_function_no_parameters",
      "void_function_with_parameters"
    ],
    "Return Value Functions": [
      "return_value_function_no_parameters",
      "return_value_function_with_parameters"
    ],
    "Lambda/Anonymous Functions": [
      "anonymous_function_no_parameters",
      "anonymous_function_with_parameters"
    ]
  },
  "functions": {
    "void_function_no_parameters": {
      "code": "function funcName() {\n    function_body();\n}"
    },
    "void_function_with_parameters": {
      "code": "function funcName(param1, param2) {\n    function_body();\n}"
    },
    "return_value_function_no_parameters": {
      "code": "function funcName() {\n    function_body();\n    return something\n}"
    },
    "return_value_function_with_parameters": {
      "code": "function funcName(param1, param2) {\n    function_body();\n    return something\n}"
    },
    "anonymous_function_no_parameters": {
      "code": "() => { function_body(); }"
    },
    "anonymous_function_with_parameters": {
      "code": "(param1, param2) => { function_body() }"
    }
  }
}
