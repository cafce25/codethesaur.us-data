      "code": "function_name() {\n    $1\n    $2\n    statements\n}\n \nfunction_name parameter1 parameter2"
    },
    "void_function_variable_parameters": {
      "code": "function_name() {\n    $@\n    statements\n}\n \nfunction_name parameter1 ... parameterN"
    },
    "return_value_function_no_parameters": {
      "code": "function_name () {\n    statements\n    return expression\n}"
    },
    "return_value_function_with_parameters": {
      "code": "function_name() {\n    $1\n    $2\n    statements\n    return expression\n}\n \nfunction_name parameter1 parameter2"
    },
    "return_value_function_variable_parameters": {
      "code": "function_name() {\n    $@\n    statements\n    return expression\n}\n \nfunction_name parameter1 ... parameterN"
