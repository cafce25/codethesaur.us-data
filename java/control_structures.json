      "code": "if (condition) {\n    statements;\n} else {\n    statements;\n}"
    },
    "if_elseif_conditional": {
      "code": "if (condition) {\n    statements;\n} else if (condition) {\n    statements;\n}"
    },
    "if_elseif_else_conditional": {
      "code": "if (condition) {\n    statements;\n} else if (condition) {\n    statements;\n} else{\n    statements;\n}"
    },
    "ternary_conditional": {
      "code": "condition ? statementIfTrue : statementIfFalse"
    },
    "switch_statement": {
      "code": "switch (expression) {\n  case constant-expression:\n    statements;\n    break;\n  default:\n    statements;\n}"
    },
    "while_loop": {
      "code": "while (condition) {\n    statements;\n}"
    },
    "do_while_loop": {
      "code": "do{\n    statements;\n}\nwhile(condition);"
    },
    "for_loop": {
      "code": "for (initialization_statements; condition; update_statements) {\n    statements;\n}"
    },
    "foreach_loop": {
