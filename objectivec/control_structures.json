    "for_loop": {
      "code": "for (initialization; condition; increment) {\n    statements;\n}",
      "name": "For loop"
    },
    "foreach_loop": {
      "code": "for (key in someDictionary) {\n    statements;\n}",
      "name": "Foreach loop"
